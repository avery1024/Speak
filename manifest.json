{
  "manifest_version": 2,
  "name": "Speak",
  "description": "Speaks highlighted text on page",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["speak.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}